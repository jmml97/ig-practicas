        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../include",
                "${workspaceRoot}/../include",
                "/usr/local/include",
                "/System/Library/Frameworks/"
            ],
            "defines": [],
